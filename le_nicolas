# TP2_RTE

import csv
import os
table = []
données = []
c = 0
with open ("/home/etudiant/Bureau/RTE_2022.csv", 'r') as fichier:
	a = csv.reader(fichier , delimiter = ",")
	for i in a :
		table.append(i)
for i in range(len(table)):
	if c % 2 == 1 :
		données.append(table[i])
		c = c+1
	else :
		table[i] = table[i]
		c = c+1
données[0] = données[1] 
données.remove(données[len(données)-1])
print(table)
